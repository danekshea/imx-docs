{
  "name": "imx-docs",
  "version": "1.33.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint . --ext .ts,.js,.jsx,.tsx --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css}'",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc",
    "typecheck-staged": "tsc-files --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx}": "yarn run lint:fix",
    "**/*.{ts,tsx}": "yarn run typecheck-staged"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-rc.1",
    "@docusaurus/plugin-google-gtag": "^2.0.0-rc.1",
    "@docusaurus/preset-classic": "^2.0.0-rc.1",
    "@docusaurus/theme-common": "^2.0.0-rc.1",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/react": "^1.6.22",
    "@stoplight/elements": "^7.5.19",
    "@svgr/webpack": "^6.2.1",
    "@types/react-router": "^5.1.18",
    "buffer": "^6.0.3",
    "clsx": "^1.1.1",
    "docusaurus2-dotenv": "^1.4.0",
    "dotenv": "^16.0.1",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^1.3.1",
    "process": "^0.11.10",
    "react": "^17.0.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.6.3",
    "url": "^0.11.0",
    "webpack": "^5.73.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@docusaurus/module-type-aliases": "^2.0.0-rc.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react-router-dom": "^6.3.0",
    "semantic-release": "^19.0.2",
    "tsc-files": "^1.1.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
